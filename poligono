object poligono
{
	var a=0
	var b=0
	var c=0 
	
	method esEquilatero()
	{
	return a==b and b==c and a==c
    }
	method esEscaleno()
	{
	 return (c<=(a+b)and b<=(a+c)and a<=(b+c))and(a!=b and b!=c and a!=c)
    }
	method esIsosceles()
	{
	return (a==b and b!=c and (c<(a+b))) or (a==c and a!=b and (b<(a+c))) or (c==b and c!=a and (a<(b+c)))
    }
}
