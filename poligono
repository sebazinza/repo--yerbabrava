object poligono
{
	var a=0
	var b=0
	var c=0 
	
	method esEquilatero()
	{
	if(a==b and a==c)
	{
		return true
	}
	else
	     {
		 return false
		 
		 }
	}
	method esEscaleno()
	{
	if((a != b) and (a != c) and (b != c))
	{
		return true
	}
	else
	    { 
		return false
		
		}
	}
	
	method esIsosceles()
	{
	if((a == b) and (c != b) and (c != a) or (a == c) and (b != a ) and (b != c) or (b == c) and (a != b) and (a != c) )
	{
		return true
	}
	else
	    { 
		return false
		
		}
	}
}
